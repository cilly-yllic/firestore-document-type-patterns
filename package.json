{
  "name": "@cilly-yllic/firestore-document-type-partterns",
  "version": "1.0.12",
  "description": "",
  "keywords": [
    "firebase",
    "firestgore",
    "security rule",
    "unit test",
    "document",
    "field type",
    "pattern"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    ">=4.2": {
      "*": [
        "dist/types/*"
      ]
    }
  },
  "engines": {
    "node": ">=18.x"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/cilly-yllic"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "test": "npm-run-all test:jest prettier:check lint",
    "lint": "eslint . --ext ts,tsx",
    "ci:update-version": "zx ./scripts/ci/update-version.mjs",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "jest": "jest --passWithNoTests --silent false --verbose false",
    "test:jest": "firebase emulators:exec --only firestore \"npm run jest\"",
    "build:clean": "shx rm -rf ./dist ./core ./types",
    "build:compile": "tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.esm5.rollup.json ./src/tsconfig.types.json",
    "build:global": "node ./scripts/make-umd-bundle.js && node ./scripts/make-closure-core.js",
    "build:package": "npm-run-all build:clean build:compile && node ./scripts/prepare-package.js && node ./scripts/generate-alias.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cilly-yllic/firestore-document-type-patterns.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cilly-yllic/firestore-document-type-patterns/issues"
  },
  "homepage": "https://github.com/cilly-yllic/firestore-document-type-patterns#readme",
  "exports": {
    ".": {
      "node": "./dist/cjs/index.js",
      "default": "./dist/esm5/index.js"
    },
    "./core": {
      "node": "./dist/cjs/core.js",
      "default": "./dist/esm5/core.js"
    },
    "./types": {
      "node": "./dist/cjs/types.js",
      "default": "./dist/esm5/types.js"
    },
    "./internal/*": {
      "node": "./dist/cjs/internal/*.js",
      "default": "./dist/esm5/internal/*.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@firebase/rules-unit-testing": "^2.0.4",
    "uuid": "^9.0.0",
    "zx": "^7.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.1302.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "klaw-sync": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  },
  "files": [
    "!**/__tests__",
    "!src/firestore/**",
    "dist/cjs/**/!(*.tsbuildinfo)",
    "dist/esm/**/!(*.tsbuildinfo)",
    "dist/esm5/**/!(*.tsbuildinfo)",
    "dist/types/**/!(*.tsbuildinfo)",
    "core",
    "types",
    "src",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
